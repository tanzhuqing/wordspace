def getArtistUrls(self):
    u = []
    for i in string.ascii_lowercase:
       url = "http://www.kuwo.cn/geci/artist_"+i+".html"
       html = requests.get(url)
       selector = etree.HTML(html)
       a = selector.xpath('//*[@id="pageDiv"]').get_absolute_url()
       print a
    return u    
   
   
   
