import requests 
import string


class spider(self):
    def __init__(self):
        print u'开始爬取内容'

    def getArtisUrls():
        u = []
        for i in string.ascii_lowercase:
            u.append("http://www.kuwo.cn/geci/artist_"+i+".html")
        return u    
        
        

     
        
  
    
    